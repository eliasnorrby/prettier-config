{
  "name": "@eliasnorrby/prettier-config",
  "version": "2.1.2",
  "description": "My prettier config",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "index.js",
    "setup.js",
    ".prettierignore"
  ],
  "bin": "setup.js",
  "main": "index.js",
  "scripts": {
    "lint": "tsc",
    "semantic-release": "semantic-release",
    "check-format": "prettier --list-different '**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}'",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}'"
  },
  "keywords": [
    "config",
    "prettier"
  ],
  "repository": "https://github.com/eliasnorrby/prettier-config",
  "author": "Elias Norrby",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@eliasnorrby/commitlint-config": "^2.0.2",
    "@eliasnorrby/semantic-release-config": "^2.0.0",
    "@types/node": "^12.7.5",
    "@types/prettier": "^1.18.2",
    "husky": "^3.0.5",
    "prettier": "^1.18.2",
    "semantic-release": "^15.13.24",
    "typescript": "^3.6.3"
  },
  "peerDependencies": {
    "prettier": "1.x"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "yargs": "^14.2.0"
  }
}
